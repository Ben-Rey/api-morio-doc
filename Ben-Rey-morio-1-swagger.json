{
  "openapi": "3.0.0",
  "info": {
    "title": "Morio-api",
    "description": "API for connecting Morio service to your back office\n\nDefault URL\\: `https://{workspace}.api.web.morio.co/developers`\n",
    "version": "1",
    "x-logo": {
      "url": "https://res.cloudinary.com/morio/image/upload/v1599656878/logo/LOGO%20MORIO/logo-morio-rvb-fond-transparent.png",
      "backgroundColor": "#FFFFFF",
      "altText": "Morio logo"
    }
  },
  "servers": [
    {
      "url": "https://{workspace}.api.web.morio.co/developers",
      "variables": {
        "workspace": {
          "description": "workspace assigned by Morio",
          "default": "demo"
        }
      }
    }
  ],
  "paths": {
    "/bike/list_devices": {
      "get": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "tags": [
          "Bike"
        ],
        "summary": "List all devices",
        "responses": {
          "200": {
            "description": "response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BikeListDevices"
                }
              }
            }
          }
        }
      }
    },
    "/bike/create-device": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "tags": [
          "Bike"
        ],
        "summary": "Add new bikes",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BikeCreationList"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/device_created_response_201"
                }
              }
            }
          },
          "400": {
            "description": "A key in the json data is missing",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/error_key_missing_response_400"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/device_created_error_response_404"
                }
              }
            }
          }
        }
      }
    },
